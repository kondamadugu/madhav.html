2.
import random


def generate_Eid():


return ''.join([str(random.randint(0, 9)) for i in range(6)])


def generate_Pid():


return ''.join([str(random.randint(0, 9)) for i in range(6)])


employee_id = generate_Eid() print("Employee ID:", employee_id)

project_id = generate_Pid() print("Project ID:", project_id)
•	This is the element used for to identify the project details
•	And this project id can gives the client details and how many users placed in the project
•	And also it will shows employees details who can placed in the project by using project id
3.
public class Employee {

String lastName = null;

String firstName = null; double ID;

public Employee(String lastName, String firstName, double ID){ this.lastName = lastName;
this.firstName = firstName; this.ID = ID;
}


public String empStat(){
return "Last Name: " + lastName + "First Name: " + firstName + "ID" + ID;
}


}
public class MainEmployee {


public static void main(String args[]){


Employee nub1 = new Employee ("Griffin", "Rama", 000001); System.out.println(nub1);
Employee nub2 = new Employee ("Griffin", "Siva", 000002); System.out.println(nub2);
Employee nub3 = new Employee ("Griffin", "Jadadeesh", 000003); System.out.println(nub3);
Employee nub4 = new Employee ("Griffin", "Vijay", 000004); System.out.println(nub4);
}
}
